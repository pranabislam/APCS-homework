import java.io.*;
import java.util.*;

public class knights{
    private boolean done;
    private int me = 1;
    private int moves = 0;
    private int visited = -2;
    private int[][] board;

    public String toString(){
	String s = "";
	for(int i = 0;i < board.length; i ++){
	    for(int b = 0; b < board[0].length; b ++){
	        s = s + String.format("%d",(board[i][b])) + " ";
	    }
	    s = s + "\n";
	}
	return s;
    }
    public void create(){
	board = new int[9][9];
        for(int i = 0; i < board[0].length; i ++){
	    board[0][i] = -1;
	    board[1][i] = -1;
	    board[board.length - 2][i] = -1;
	    board[board.length - 1][i] = -1;
	}
	for(int i = 1; i < board[0].length - 1; i ++){
	    board[i][0] = -1;
	    board[i][1] = -1;
	    board[i][board.length - 2] = -1;
	    board[i][board.length - 1] = -1;
	    
	}
	

    }
    
    public boolean check(){
	done = true;
	for(int i = 0; i < board.length; i ++){
	    for(int b = 0;b <board[0].length; b ++){
		if(board[i][b] == 0){
		    done = false;
		}
	    }
	}
	return done;
    }
    
    public void delay(int n){
	try {
	    Thread.sleep(n);
	} catch (Exception e) {}
    }
    
    public void tour(int x, int y){
	if(board[x][y] == -1 || board[x][y] > 0 || board[x][y] == visited || check()){
	    return;
	}
	if(check()){
	    System.out.println(this);
	}
	delay(100);
	System.out.println(this);
	board[x][y] = me;
	me = me + 1;
        tour(x + 1, y + 2);
        tour(x - 1, y + 2);
        tour(x + 1, y - 2);
        tour(x - 1, y - 2);
        tour(x + 2, y + 1);
	tour(x + 2, y - 1);
	tour(x - 2, y + 1);
	tour(x - 2, y - 1);
	if(!done){
	    board[x][y] = 0;
	}
    }
    
    public static void main(String[] args){
	knights a  = new knights();
	a.create();
	System.out.println(a);
	a.tour(3,3);
	
    }



}
